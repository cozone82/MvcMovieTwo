@model IEnumerable<Movie>

@{
    ViewData["Title"] = "All User's Movies";
}


<div class="movie-panel">
    <div>
        @ViewData["Title"]
    </div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Release Date</th>
                <th>Genre</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var movie in Model )
            {
                <tr>
                    <td>@movie.Title</td>
                    <td>@movie.ReleaseDate</td>
                    <td>@movie.Genre</td>
                    <td>@movie.Price</td>
                </tr>
            }
        </tbody>
    </table>
</div>